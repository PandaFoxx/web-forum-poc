{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "WebForumApi",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/api/WebForumApi/bin/Debug/net9.0/WebForumApi.dll",
      "args": [],
      "cwd": "${workspaceFolder}/api/WebForumApi",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)",
        "uriFormat": "%s"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_URLS": "https://localhost:7098;http://localhost:5254"
      },
      "console": "internalConsole",
      "logging": {
        "moduleLoad": false
      }
    }
  ]
}
